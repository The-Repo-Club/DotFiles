#!/usr/bin/env bash

desktop() {
    if [[ ! -z "$XDG_CURRENT_DESKTOP" ]]; then
        echo  "$XDG_CURRENT_DESKTOP"
    elif [[ ! -z "$XDG_SESSION_DESKTOP" ]]; then
        echo  "$XDG_SESSION_DESKTOP"
    elif [[ ! -z "$DESKTOP_SESSION" ]]; then
        echo  "$DESKTOP_SESSION"
    else
        echo "Can not detech desktop"
    fi
}

desktop
